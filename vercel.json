{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "/backend/main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "/backend/main.py"
        },
        {
            "src": "/docs",
            "dest": "/backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}